{
  "name": "joi-browserify",
  "version": "0.0.1",
  "description": "Shim to use Joi in the browser with Browserify",
  "browser": "joi-browserify.min.js",
  "scripts": {
    "build": "node ./build.js",
    "test": "npm run build && node_modules/lab/bin/lab",
    "preinstall": "npm install --ignore-scripts",
    "postinstall": "npm run build"
  },
  "keywords": [
    "joi",
    "browserify"
  ],
  "author": "Frederic Hemberger (http://frederic-hemberger.de/)",
  "repository": {
    "type": "git",
    "url": "git://github.com/fhemberger/joi-browserify.git"
  },
  "license": "MIT",
  "dependencies": {
    "aliasify": "^1.5.1",
    "browserify": "^8.1.3",
    "joi": "5.x.x",
    "minifyify": "^6.1.0",
    "npm": "^2.5.0"
  },
  "devDependencies": {
    "code": "^1.3.0",
    "lab": "^5.2.1"
  }
}
